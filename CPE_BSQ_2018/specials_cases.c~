#include <stdlib.h>
#include "include/my.h"

int first_square(char **map, int nb_rows, int nb_cols)
{
    int i = 0;

    for (; map[1][i] != '\0'; i++) {
        if (map[1][i] == '.');
    }
    change_square(map, 1, i, 1);
    disp_square(map, nb_rows);
    exit(0);
}
